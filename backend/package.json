  "keywords": [
    "API",
    "REST",
    "Biomedicina",
    "UNISUL",
    "Express",
    "MongoDB",
    "nodejs"
  ],
  "author": "Pedro Victor Castro de Sousa",
  "email": "pedrovicstro@gmail.com",
  "license": "ISC",
  "description": "RESTful API para gestão acadêmica e de networking na área da biomedicina estética.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mmyersbyte/labem"
  },
  "homepage": "https://labemunisul.com.br",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.1",
    "multer": "^1.4.5-lts.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^24.0.4",
    "nodemon": "^3.1.10",
    "poku": "^3.0.2",
    "typescript": "^5.8.3"
  }
